<?xml version="1.0"?>
<package >
    <metadata>
        <id>Geta.EpiCategories</id>
        <version>$version$</version>
        <title>Geta.EpiCategories</title>
        <authors>Geta</authors>
        <owners>Geta</owners>
        <projectUrl>https://github.com/Geta/EpiCategories</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
