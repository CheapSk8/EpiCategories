        <releaseNotes></releaseNotes>
        <copyright></copyright>
        <tags>Episerver, Geta</tags>
        <dependencies>
            <dependency id="EPiServer.Framework" version="10.0.1" />
            <dependency id="EPiServer.CMS.Core" version="10.0.1" />
            <dependency id="EPiServer.CMS.UI.Core" version="10.0.2" />
        </dependencies>
    </metadata>
    <files>
        <file src="bin\$configuration$\Geta.EpiCategories.dll" target="lib" />
        <file src="modules\**\*.*" target="content\modules" />
    </files>
</package>